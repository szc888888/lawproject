<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.dao.CamilleDao">
    
    	<!-- 根据卡密号拆查询卡密信息 -->
    <select id="getCamilleByNumber" resultType="io.renren.entity.CamilleEntity" parameterType="string">
        select
         *
         from tb_camille where camille_number = #{camilleNumber}
    </select>


</mapper>