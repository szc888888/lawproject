<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.dao.FlagstudioDao">

    <resultMap type="io.renren.entity.FlagstudioEntity" id="flagstudioMap">
        <result property="id" column="id"/>
        <result property="apiKey" column="api_key"/>
        <result property="token" column="token"/>
        <result property="tokenTime" column="token_time"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
        <result property="dayCount" column="day_count"/>
    </resultMap>
    <select id="getListByTodayCountLt500" resultType="io.renren.entity.FlagstudioEntity">
        SELECT * from tb_flagstudio where `status` = 0 AND day_count &lt; 500
    </select>


</mapper>